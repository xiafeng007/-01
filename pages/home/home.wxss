#banner_btn{
  position: fixed;
  bottom:70rpx;
  right:0;
  left:0;
  margin:auto;
  z-index: 9999;
  width:250rpx;
  height:75rpx;
}
 
swiper-container{  
  position: relative;  
}  
.swiper-container .swiper{  
  height: 100vh;  
}  
.swiper-container .swiper .img{  
  width: 100%;  
  height: 100%;  
}  
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 30rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 15rpx;  
  width: 15rpx;  
  height: 15rpx;
  transform: rotate(45deg);  
  background: #DDD;  
  transition: all .6s;
}  
.swiper-container .dots .dot.active{  
  background:#F4E03A;  
}  